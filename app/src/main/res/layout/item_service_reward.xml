<?xml version="1.0" encoding="utf-8"?>
<com.miyin.zhenbaoqi.widget.LineFrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="@dimen/dp_80"
    android:background="@color/white">

    <com.miyin.zhenbaoqi.widget.CircleImageView
        android:id="@+id/iv_avatar"
        android:layout_width="@dimen/dp_41"
        android:layout_height="@dimen/dp_41"
        android:layout_marginStart="@dimen/dp_11"
        android:layout_marginTop="@dimen/dp_17" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/tv_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/dp_65"
        android:layout_marginTop="@dimen/dp_24"
        android:includeFontPadding="false"
        android:textColor="#191919"
        android:textSize="@dimen/sp_13"
        tools:text="一只小猴子" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/tv_phone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/dp_65"
        android:layout_marginTop="@dimen/dp_47"
        android:includeFontPadding="false"
        android:textColor="@color/text_33"
        android:textSize="@dimen/sp_12"
        tools:text="19568954578（高级玩家）" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/tv_price"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="end"
        android:layout_marginEnd="@dimen/dp_17"
        android:layout_marginTop="@dimen/dp_24"
        android:includeFontPadding="false"
        android:textColor="#E02A2A"
        android:textSize="@dimen/sp_13"
        tools:text="0.00" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/tv_count"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="end"
        android:layout_marginEnd="@dimen/dp_17"
        android:layout_marginTop="@dimen/dp_47"
        android:includeFontPadding="false"
        android:textColor="@color/text_33"
        android:textSize="@dimen/sp_12"
        tools:text="1人" />

</com.miyin.zhenbaoqi.widget.LineFrameLayout>
